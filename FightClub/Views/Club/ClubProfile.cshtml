@model Club

@{
    Layout = "_Layout";
    ViewBag.Title = "Club profile";
}

<div style="margin: 10px">
    <div>Name: @Model.Name</div>
    <div>Id: @Model.Id</div>
    <div>Creater: @ViewBag.NameCreaterOfClub</div>
    <a asp-action="ChattingRoom" asp-controller="Chat" asp-route-chatId="@Model.ChatClub.Id">Chat</a>
</div>